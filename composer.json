{
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require-dev": {
        "phpstan/phpstan": "1.4.*",
        "rector/rector": "0.12.*",
        "symfony/phpunit-bridge": "6.0.*"
    }
}
